@define refs {
  name: tedirThumbnail;
}

@alga {refs.name} {
  .{refs.name} {
  
    .dropZoneWrap {
      
      label.{refs.name}Label {
        ref: pt-0px pb-0px;
      }
      
      .dropZoneBody p .dropZoneTitle {
        ref: fg-inherit;
      }
    }
    
    &Image {
      ref: objectFit-fill w-100pct;
    }
  }
}
