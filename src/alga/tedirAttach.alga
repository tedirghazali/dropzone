@define refs {
  name: tedirAttach;
}

@alga {refs.name} {
  .{refs.name} {
    ref: padding-0px maxWidth-none maxHeight-none display-inlineGrid;
  
    .dropZoneWrap {
      ref: border-none boxShadow-none;
      
      label.{refs.name}Label {
        ref: display-inlineGrid padding-0px;
      }
    }
  }
}
